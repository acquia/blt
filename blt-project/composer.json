{
    "name": "acquia/blt-project",
    "description": "Example project used to create new projects with BLT.",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Matthew Grasmick",
            "email": "matthew.grasmick@acquia.com"
        }
    ],
    "repositories": {
        "blt": {
            "type": "path",
            "url": "../blt",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "acquia/blt": "*@dev"
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "vendor/acquia/blt/composer.include.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
